{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "removeComments": true,
    "noEmitOnError": true,
    "noUnusedLocals": true,
    "strictNullChecks": false,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ]
}
